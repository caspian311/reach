<div class="title"> 
  <%= @title %>: <%= @map_name %>
</div> 
<div class="content"> 

<table border="1">
<tr>
   <th>Player Name</th>
   <th>Map</th>
   <th>Effectiveness</th>
</tr>
<% @player_stats.each do |player_stat| %>
<tr>
   <td><%= player_stat.player_name %></td>
   <td><%= player_stat.map %></td>
   <td><%= player_stat.effectiveness %></td>
</tr>
<% end %>
</table>

<%= link_to "Back", "/player_effectiveness#index" %>

</div>
